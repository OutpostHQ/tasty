{
  "name": "tastycss",
  "version": "0.8.2",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "sideEffects": false,
  "repository": "OutpostHQ/tatsy",
  "homepage": "https://tasty.numl.design/",
  "bugs": "https://github.com/OutpostHQ/tasty/issues",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "component",
    "components",
    "create",
    "css",
    "css-in-js",
    "global",
    "globals",
    "javascript",
    "js",
    "nesting",
    "object",
    "object-oriented",
    "oo",
    "oocss",
    "oriented",
    "scope",
    "scoped",
    "ssr",
    "ssg",
    "style",
    "styled",
    "styles",
    "stylesheet",
    "stylesheets",
    "style-to-state",
    "tasty",
    "tastycss",
    "theme",
    "themes",
    "theming",
    "token",
    "tokens",
    "type",
    "typed",
    "types",
    "transform",
    "ts",
    "variable",
    "variables"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react && yarn size-limit",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "test": "jest"
  },
  "devDependencies": {
    "@jenga-ui/button": "^0.1.1",
    "@jenga-ui/core": "^0.1.1",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "^12.1.2",
    "@typescript-eslint/eslint-plugin": "5.8.1",
    "@typescript-eslint/parser": "5.8.1",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react-app": "7.0.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "jest": "^27.5.1",
    "prettier": "2.5.1",
    "tsup": "^6.2.3",
    "typescript": "4.5.4"
  },
  "dependencies": {
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2",
    "react-is": "^17.0.0",
    "styled-components": "5.3.0"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari major versions"
  ]
}
