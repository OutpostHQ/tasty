{
  "name": "tastycss",
  "version": "0.13.0",
  "keywords": [
    "component",
    "components",
    "create",
    "css",
    "css-in-js",
    "global",
    "globals",
    "javascript",
    "js",
    "jsx",
    "nesting",
    "next",
    "nextjs",
    "object",
    "object-oriented",
    "oo",
    "oocss",
    "oriented",
    "preact",
    "scope",
    "scoped",
    "ssr",
    "ssg",
    "style",
    "styled",
    "styles",
    "stylesheet",
    "stylesheets",
    "style-to-state",
    "tasty",
    "tastycss",
    "theme",
    "themes",
    "theming",
    "token",
    "tokens",
    "type",
