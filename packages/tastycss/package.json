  ],
  "homepage": "https://github.com/OutpostHQ/tasty",
  "bugs": "https://github.com/OutpostHQ/tasty/issues",
  "repository": "OutpostHQ/tasty",
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --config ./tsup.config.ts",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup src/index.ts --minify --treeshake --format esm,cjs --watch --dts --external react && yarn size-limit",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "test": "jest"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari major versions"
  ],
  "dependencies": {
    "react-is": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/node": "18.14.4",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "^29.4.3",
    "prettier": "2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "5.3.8",
    "tsup": "6.6.3",
    "typescript": "~4.9.5"
  },
  "peerDependencies": {
    "react": ">= 17.0.0",
    "react-dom": ">= 17.0.0",
    "styled-components": ">= 5.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
